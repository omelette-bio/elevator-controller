										<BinaryOp name="4" operator="&lt;&gt;">
											<operands>
												<IdExpression>
													<path>
														<ConstVarRef name="oFloorNumber"/>
													</path>
												</IdExpression>
												<IdExpression>
													<path>
														<ConstVarRef name="iDestinationFloorNumber"/>
													</path>
												</IdExpression>
											</operands>
										</BinaryOp>
									</operands>
								</NAryOp>
							</condition>
							<pragmas>
								<ed:Transition oid="!ed/4bf7/2C51/4478/681bc0fb5417"/>
							</pragmas>
						</Transition>
					</until>
					<pragmas>
						<ed:State oid="!ed/4a49/2C51/4478/681bbe28a29"/>
