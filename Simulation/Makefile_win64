SHELL=cmd.exe

C_FLAGS = -pedantic

all: Proof__Proof.exe Proof__Proof.dll 

win64/Proof_Proof.o: Proof_Proof.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "Proof_Proof.c" -o "win64\Proof_Proof.o"

